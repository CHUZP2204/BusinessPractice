

@{
    ViewBag.Title = "UnauthorizeOperation";
    Layout = "~/Views/Shared/_Layout.cshtml";
}



<section class="content-header">
    <h2>Error Operacion No Permitida</h2>
</section>

<section class="content">
    <div class="error-page">
        <h2 class="headline text-red">
            Ingreso No Permitido a Empleados<br />
            Estas Conectado como @ViewBag.tipoUsuario
        </h2>
        <div class="error-content">
            <h3><i class="fa fa-warning text-red">Sin Permiso</i></h3>
            <p>Consulta Al Administrador Sobre Este Problema</p>
        </div>
    </div>
</section>

<!-- Modal -->
<div class="modal fade" id="myModalError" data-bs-backdrop="static" tabindex="-1" aria-labelledby="exampleModalCenteredScrollableTitle" style="display: none;" aria-hidden="true">
    <div class="modal-dialog modal-dialog-centered modal-dialog-scrollable">
        <div class="modal-content">
            <div class="modal-header" style="text-align:center">
                <h5 class="modal-title" id="exampleModalCenteredScrollableTitle" style="margin-left: 100px;color:red">
                    <i class="fa fa-exclamation-triangle"></i>
                    Se Produjo un Error
                    <i class="fa fa-exclamation-triangle"></i>
                </h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body" style="text-align:center">
                <h2 class="headline text-red">
                    Ingreso No Autorizado A Empleados<br />
                    Estas Conectado Como @ViewBag.tipoUsuario
                    Intentente De Nuevo
                </h2>
                <p>O bien Consulte Al Admnistrador Sobre Este Problema</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Cerrar</button>
            </div>
        </div>
    </div>
</div>


@section scripts{

    <script src="~/Scripts/Sesion/llamarModal.js"></script>
}
